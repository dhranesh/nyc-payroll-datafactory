{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dhraneshfactory"
		},
		"AzureBlobFS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureBlobFS'"
		},
		"AzureSQLDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQLDatabase'"
		},
		"AzureBlobFS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dhranespayroll.dfs.core.windows.net/"
		},
		"AzureSQLDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dhraneshsql.database.windows.net"
		},
		"AzureSQLDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ncpayroll"
		},
		"AzureSQLDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dhraneshsql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dhraneshfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobFS_accountKey": {
						"value": "[parameters('AzureBlobFS_accountKey')]"
					},
					"AzureSQLDatabase_password": {
						"value": "[parameters('AzureSQLDatabase_password')]"
					},
					"AzureBlobFS_properties_typeProperties_url": {
						"value": "[parameters('AzureBlobFS_properties_typeProperties_url')]"
					},
					"AzureSQLDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_server')]"
					},
					"AzureSQLDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_database')]"
					},
					"AzureSQLDatabase_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}